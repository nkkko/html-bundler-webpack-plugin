<!DOCTYPE html>
<html>
<head>
  <title>{{ title }}</title>
  <link href="@images/favicons/favicon.svg" rel="icon" type="image/svg">
  <script disable-src="app.js"></script>
</head>
<body>
  {{> header }}
  {{> menu/nav }}
  <h1>{{ headline }}</h1>
  <h1>{{ articleTitle }}</h1>
  <ul class="articles">
    {{#each articles}}
    <li>{{this.name}}: {{this.price}}</li>
    {{/each}}
  </ul>
  {{> footer }}
</body>
</html>
